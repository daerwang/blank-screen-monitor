{
  "name": "@qruntime/blank-screen-monitor",
  "version": "0.0.9",
  "location": "/dist",
  "description": "Q Runtime JSBridge",
  "main": "dist/index.js",
  "scripts": {
    "start": "webpack-dev-server --mode=development --qappenv=qa",
    "start:cocosdemo": "webpack-dev-server --mode=development --qappenv=qa --cocosdemo",
    "build:qa": "webpack --mode=development --qappenv=qa --dist",
    "build:qa.bak": "webpack --mode=development --qappenv=qa --dist && cp -rf ./public/* ./dist/qa/",
    "build:pre": "webpack --mode=development --qappenv=pre --dist",
    "build:prod": "webpack --dist",
    "build:demo": "webpack --mode=development --qappenv=qa --demo",
    "build:cocos": "webpack --mode=development --cocos",
    "build": "npm run clean && npm run build:qa && npm run build:pre && npm run build:prod",
    "clean": "rimraf ./dist",
    "release": "node ./bin/deploy.js",
    "lint": "lint-staged .",
    "doc": "docsify serve docs",
    "analyze": "webpack --profile --json > stats.json",
    "commitmsg": "validate-commit-msg",
    "changelog": "conventional-changelog -p angular -i CHANGLOG.md -s -r 0"
  },
  "repository": {
    "type": "git",
    "url": "git@git.qutoutiao.net:wide-fe/blank-screen-monitor.git"
  },
  "publishConfig": {
    "registry": "http://nexus.qutoutiao.net/repository/fe/"
  },
  "author": "Qutoutiao Inc.",
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-object-assign": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@qapp/qapp-bridge": "0.8.8-beta-6",
    "antd-mobile": "^2.3.3",
    "axios": "^0.19.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.13.0",
    "chai": "^4.0.2",
    "conventional-changelog-cli": "^2.1.0",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^3.6.0",
    "docsify-cli": "^4.4.1",
    "eslint": "^7.4.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^6.0.0",
    "form-data": "^3.0.0",
    "husky": "^4.2.5",
    "json-loader": "^0.5.7",
    "lint-staged": "^10.2.11",
    "mocha": "^8.0.1",
    "prettier": "^2.0.5",
    "prettier-eslint-cli": "^5.0.0",
    "pretty-quick": "^2.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
    "style-loader": "^1.2.1",
    "url-loader": "^4.1.0",
    "validate-commit-msg": "^2.14.0",
    "vconsole": "^3.3.4",
    "vue": "^2.6.11",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.4",
    "@babel/runtime-corejs2": "^7.10.4"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged && lint-staged"
    }
  }
}
